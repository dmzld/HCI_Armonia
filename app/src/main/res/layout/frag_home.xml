<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/동아리게시판"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="150dp"
        android:layout_marginTop="40dp"
        android:text="동아리게시판" />

    <ListView
        android:id="@+id/listHomeClubView"
        android:layout_width="match_parent"
        android:layout_height="180dp"
        android:layout_marginTop="80dp" />

    <TextView
        android:id="@+id/union_club_baord_textwiev"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="160dp"
        android:layout_marginTop="320dp"
        android:text="자유게시판" />

    <ListView
        android:id="@+id/listHomeFreeView"
        android:layout_width="match_parent"
        android:layout_height="180dp"
        android:layout_marginTop="350dp" />

    <Button
        android:id="@+id/btnMoreFree"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/union_club_baord_textwiev"
        android:layout_alignParentEnd="true"
        android:text="더보기" />


</RelativeLayout>
